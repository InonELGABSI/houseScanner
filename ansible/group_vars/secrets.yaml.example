# HouseScanner Secrets Configuration
# Copy this file to secrets.yaml and update with your actual credentials
# Then encrypt it with: ansible-vault encrypt secrets.yaml

# Database Password
db_password: your_secure_db_password_here

# DockerHub Credentials
dockerhub_username: your_dockerhub_username
dockerhub_password: your_dockerhub_password_or_access_token

# AWS Credentials (if not using AWS CLI profile)
# aws_access_key: YOUR_AWS_ACCESS_KEY
# aws_secret_key: YOUR_AWS_SECRET_KEY
