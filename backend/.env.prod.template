NODE_ENV=production
PORT=3000
GLOBAL_PREFIX=api

# CORS Settings (Ansible will replace YOUR_EC2_DOMAIN)
CORS_ORIGINS=https://YOUR_EC2_DOMAIN,http://YOUR_EC2_DOMAIN:8080,http://YOUR_EC2_DOMAIN

# Database Configuration - RDS PostgreSQL (Ansible will replace these)
DATABASE_URL=postgresql://your-db-username:your-db-password@YOUR_RDS_ENDPOINT:5432/your-db-name

# JWT Configuration (Ansible will replace)
JWT_SECRET=your-jwt-secret-here
JWT_ISSUER=house-scanner
JWT_AUDIENCE=house-scanner-users
JWT_ACCESS_TTL=15m
JWT_REFRESH_TTL=7d

# AWS Configuration for S3 Image Storage (Ansible will replace)
AWS_REGION=your-aws-region
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_S3_BUCKET=your-s3-bucket-name
STORAGE_BUCKET=your-s3-bucket-name
STORAGE_REGION=your-aws-region
STORAGE_USE_SSL=true
STORAGE_PUBLIC_URL=https://your-s3-bucket-name.s3.amazonaws.com
STORAGE_ENDPOINT=https://s3.your-aws-region.amazonaws.com
STORAGE_ACCESS_KEY=your-aws-access-key
STORAGE_SECRET_KEY=your-aws-secret-key

# Backend API URL (Ansible will replace)
BACKEND_API_URL=http://YOUR_EC2_DOMAIN:3000

# Agents Service URL (Ansible will replace)
AGENTS_SERVICE_URL=http://YOUR_EC2_DOMAIN:8000

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# BullMQ Configuration
BULLMQ_REDIS_HOST=redis
BULLMQ_REDIS_PORT=6379
BULLMQ_REDIS_DB=0
