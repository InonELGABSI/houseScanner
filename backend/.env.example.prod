NODE_ENV=production
PORT=3000
GLOBAL_PREFIX=api
CORS_ORIGINS=https://yourdomain.com
DATABASE_URL=postgresql://username:password@your-rds-instance.region.rds.amazonaws.com:5432/house_scanner
JWT_SECRET=your-strong-random-secret-here-change-this
JWT_ISSUER=house-scanner
JWT_AUDIENCE=house-scanner-users
JWT_ACCESS_TTL=15m
JWT_REFRESH_TTL=7d
AWS_REGION=us-east-1
